{
  "SignalHubUri": "<secret>",
  "Kafka": {
    "ConsumerConfig": {
      "BootstrapServers": "<secret>",
      "ClientId": "DMS.RealTimeDataGenerator",
      "GroupId": "monitoring"
    },
    "Topic": "realtimedata"
  },
  "InfluxDB": {
    "Token": "<secret>",
    "Bucket": "data",
    "Org": "ENI-SoftwareSystems"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "RealTimeDataConsumer"
    }
  }
}